ountList = this.musics.length;\r\n      this.index + 1 >= countList ? this.index : this.index++;\r\n      if (this.current !== undefined) {\r\n        this.player.src = this.current.src;\r\n      }\r\n      this.isPause = false;\r\n    },\r\n    previous() {\r\n      this.index <= 0 ? 0 : this.index--;\r\n      this.player.src =\r\n        this.current.src !== undefined ? this.current.src : this.player.src;\r\n      this.isPause = false;\r\n    }\r\n  },\r\n  /*  created() {\r\n    this.current = this.musics[this.index];\r\n    this.player.src = this.current.src;\r\n  } */\r\n  mounted() {\r\n    this.player.src = this.current.src;\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n\r\n.card {\r\n  margin-right: 2px;\r\n}\r\n.iconbtn {\r\n  width: 40px;\r\n  height: 40px;\r\n  margin: 20px;\r\n  position: relative;\r\n  border-radius: 50%;\r\n  cursor: pointer;\r\n}\r\n\r\nbody {\r\n  font-family: Arial, Helvetica, sans-serif;\r\n}\r\n\r\n.s1 {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.s1 .iconbtn {\r\n  border: solid 4px;\r\n}\r\n.span {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-top: 4px;\r\n  font-size: 33px;\r\n  border-radius: 47px;\r\n}\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nheader {\r\n  display: flex;\r\n  justify-content: center;\r\n  background-color: #000;\r\n  margin: 0px;\r\n  color: #fff;\r\n  padding: 10px;\r\n  margin-bottom: 50px;\r\n}\r\n.center {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n#artistSpan {\r\n  margin-top: 5px;\r\n  margin-left: 5px;\r\n}\r\n</style>\r\n\r\n"]}]}